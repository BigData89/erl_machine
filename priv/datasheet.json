{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Datasheet",
    "description": "A document that specifies characteristics of an extension",
    "type": "object",
    "properties":{
        "serial_no":{
            "type": "string",
            "description": "The unique assembly runtime identifier which is assigned incrementally"
        },
        "body":{
            "type": "object",
            "description": "The initial state of an extension"
        },
        "model_no":{
            "type": "string",
            "description": ""
        },
        "socket":{
            "description": "Rotation interface"
        },
        "model":{
            "type": "object",
            "properties":{
                "name":{
                    "type": "string",
                    "description": "Domain level implementation (module name)"
                },
                "options":{
                    "type": "array",
                    "description": "Domain layer options"
                }
            },
            "required": [ "name", "options" ],
            "description": ""
        },
        "prototype":{
            "type": "object",
            "properties":{
                "name": {
                    "type": "string",
                    "description": "Service level implementation (module name)"
                },
                "options": {
                    "type": "array",
                    "description": "Service layer options"
                }
            },
            "required": [ "name", "options" ],
            "description": ""
        },
        "tags":{
            "type": "array",
            "description": ""
        },
        "label":{
            "description": ""
        },
        "part_no":{
            "type": "string",
            "description": ""
        },
        "env":{
            "type": "object",
            "description": "Execution context"
        },
        "desc":{
            "type": "string",
            "description": "Short overview of the exrtension's role within overall system"
        }
    },
    "required": [ "model" ]
}
