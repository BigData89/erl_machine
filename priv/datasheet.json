{
    "$schema": "http://json-schema.org/draft-04/schema#",
    "title": "Datasheet",
    "description": "A document that summarizes characteristics of an extension",
    "type": "object",
    "properties":{
        "serial_no":{
            "type": "string",
            "description": "Runtime identifier"
        },
        "body":{
            "type": "object",
            "description": "The initial state"
        },
        "model_no":{
            "type": "string",
            "description": ""
        },
        "socket":{
            "description": "Processing interface"
        },
        "schema":{
            "properties":{
                "vertices":{
                    "type": "object",
                    "additionalProperties":{
                        "$ref": "#"
                    }
                },
                "edges":{
                    "type": "object",
                    "additionalProperties":{
                        "anyOf": [{"type": "array", "items": { "type": "string" } }, {"type": "string"}]
                    }
                }
            },
            "additionalProperties": false,
            "description": "Transmission specification on a graph"
        },
        "model":{
            "type": "object",
            "properties":{
                "name":{
                    "type": "string",
                    "description": "Domain level module"
                },
                "options":{
                    "type": "array",
                    "description": "Domain level options"
                }
            },
            "additionalProperties": false,
            "required": [ "name", "options" ],
            "description": ""
        },
        "prototype":{
            "type": "object",
            "properties":{
                "name": {
                    "type": "string",
                    "description": "Service level module"
                },
                "options": {
                    "type": "array",
                    "description": "Service level options"
                }
            },
            "additionalProperties": false,
            "required": [ "name", "options" ],
            "description": ""
        },
        "tags":{
            "type": "array",
            "description": ""
        },
        "vertex":{
            "description": "The identity on a graph"
        },
        "part_no":{
            "type": "string",
            "description": ""
        },
        "env":{
            "type": "object",
            "description": "Execution context"
        },
        "description":{
            "type": "string",
            "description": "Short overview of the role of an extension within overall system"
        }
    },
    "required": [ "model" ]
}
